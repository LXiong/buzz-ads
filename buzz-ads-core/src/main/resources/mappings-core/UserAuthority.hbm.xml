<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.buzzinate.buzzads.domain.UserAuthority" table="bfa_user_authority">
        <id name="userId" type="int" column="USER_ID">
            <generator class="assigned" />
        </id>
        <property name="channelTarget" >
            <column name="CHANNEL_TARGET" />
        </property>
    </class>
</hibernate-mapping>
