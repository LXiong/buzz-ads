<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.buzzinate.buzzads.domain.PublisherSiteConfig" table="publisher_site_config">
        <id name="uuidBytes" type="binary" column="UUID">
            <generator class="assigned" />
        </id>
        
        <property name="blackKeywords">
            <column name="BLACK_KEYWORDS" />
        </property>
    </class>
</hibernate-mapping>