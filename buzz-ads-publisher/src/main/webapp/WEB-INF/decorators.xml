<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/ifr/*</pattern>
        <pattern>/iframe*</pattern>
        <pattern>/api/*</pattern>
        <pattern>/loginCallback*</pattern>
        <pattern>/admin/channel/thumbupload</pattern>
    </excludes>
    <decorator name="adminMain" page="AdminMain.jsp">
        <pattern>/admin*</pattern>
        <pattern>/settlement*</pattern>
    </decorator>
    <decorator name="publisherMain" page="PublisherMain.jsp">
        <pattern>/publisher*</pattern>
    </decorator>
	<decorator name="FinanceMain" page="FinanceMain.jsp">
        <pattern>/finance*</pattern>
    </decorator>
    <decorator name="default" page="Main.jsp">
        <pattern>/*</pattern>
    </decorator>
</decorators>
